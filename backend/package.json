{
  "name": "fomo-backend",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "dev:node": "tsx watch src/server.ts",
    "dev:python": "cd /app/backend && python server.py",
    "dev:parser": "cd /app/twitter-parser-v2 && yarn dev",
    "dev:all": "concurrently -k -n PYTHON,NODE,PARSER -c blue,green,yellow \"yarn dev:python\" \"yarn dev:node\" \"yarn dev:parser\"",
    "dev:fractal": "FRACTAL_ONLY=1 MINIMAL_BOOT=1 FRACTAL_ENABLED=true tsx watch src/server.ts",
    "fractal": "tsx watch src/app.fractal.ts",
    "fractal:isolated": "tsx src/app.fractal.ts",
    "start": "node dist/server.js",
    "start:fractal": "FRACTAL_ONLY=1 MINIMAL_BOOT=1 FRACTAL_ENABLED=true node dist/server.js",
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "guard:fractal": "tsx scripts/fractal_import_guard.ts",
    "test:ci": "npm run guard:fractal && npm run test"
  },
  "dependencies": {
    "@fastify/cors": "^9.0.1",
    "@fastify/websocket": "^10.0.1",
    "@types/jsonwebtoken": "^9.0.10",
    "adm-zip": "^0.5.16",
    "axios": "^1.13.2",
    "bottleneck": "^2.19.5",
    "csv-parse": "^6.1.0",
    "debug": "^4.4.3",
    "density-clustering": "^1.3.0",
    "dotenv": "^17.3.1",
    "ethers": "^6.0.0",
    "fastify": "^4.28.1",
    "https-proxy-agent": "^7.0.6",
    "ioredis": "^5.9.2",
    "jsonwebtoken": "^9.0.3",
    "mongoose": "^8.8.0",
    "node-cron": "^4.2.1",
    "technicalindicators": "^3.1.0",
    "uuid": "^13.0.0",
    "ws": "^8.18.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^11.0.0",
    "@types/ws": "^8.5.13",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "@vitest/ui": "^4.0.18",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "pino-pretty": "^13.1.3",
    "tsx": "^4.21.0",
    "typescript": "^5.6.3",
    "vitest": "^4.0.18"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
